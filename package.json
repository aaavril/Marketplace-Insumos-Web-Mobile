{
  "name": "marketplace-insumos-web-mobile",
  "version": "1.0.0",
  "private": true,
  "description": "Marketplace de Insumos - Monorepo para Web y Mobile",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=apps/web",
    "build:web": "npm run build --workspace=apps/web",
    "lint:web": "npm run lint --workspace=apps/web",
    "preview:web": "npm run preview --workspace=apps/web",
    "start:mobile": "npm run start --workspace=apps/mobile",
    "android:mobile": "npm run android --workspace=apps/mobile",
    "ios:mobile": "npm run ios --workspace=apps/mobile",
    "lint": "eslint .",
    "install:all": "npm install"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.9.4"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

